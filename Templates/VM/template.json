{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_ProjetVM_name": {
            "defaultValue": "ProjetVM",
            "type": "String"
        },
        "disks_ProjetVM_OsDisk_1_e138680a83f24337b1bc17cb20ddaa04_externalid": {
            "defaultValue": "/subscriptions/1ccdf286-b4fe-4830-a625-347d40ad11b9/resourceGroups/LeProjet/providers/Microsoft.Compute/disks/ProjetVM_OsDisk_1_e138680a83f24337b1bc17cb20ddaa04",
            "type": "String"
        },
        "networkInterfaces_projetvm349_externalid": {
            "defaultValue": "/subscriptions/1ccdf286-b4fe-4830-a625-347d40ad11b9/resourceGroups/LeProjet/providers/Microsoft.Network/networkInterfaces/projetvm349",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2019-07-01",
            "name": "[parameters('virtualMachines_ProjetVM_name')]",
            "location": "francecentral",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_B2s"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "MicrosoftWindowsDesktop",
                        "offer": "Windows-10",
                        "sku": "rs5-pro",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Windows",
                        "name": "[concat(parameters('virtualMachines_ProjetVM_name'), '_OsDisk_1_e138680a83f24337b1bc17cb20ddaa04')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "id": "[parameters('disks_ProjetVM_OsDisk_1_e138680a83f24337b1bc17cb20ddaa04_externalid')]"
                        }
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_ProjetVM_name')]",
                    "adminUsername": "Estiam",
                    "windowsConfiguration": {
                        "provisionVMAgent": true,
                        "enableAutomaticUpdates": true
                    },
                    "secrets": [],
                    "allowExtensionOperations": true,
                    "requireGuestProvisionSignal": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_projetvm349_externalid')]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true
                    }
                },
                "licenseType": "Windows_Client"
            }
        }
    ]
}